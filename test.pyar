دالة اختبار():
    اطبع('مرحبا دالة ')

س = ٥
ص = ٣

طالما س < ١٠٠:
    س += س/10

    إذا كان س - قرب(س) < ٠.٠٩:
        استمر
    وإلا إذا كان س - قرب(س) > ٠.٩:
        اطبع("""***""")

    اطبع(س, " >>>")
    لكل م في مدى(رقم صحيح(س)):
        اطبع(م)

إذا كان س > ص:
    اطبع("س أكبر من ص")
وإلا:
    اطبع("ص أكبر من س")

